<!DOCTYPE html>
<html lang="pt_br">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Document</title>
        
        <style>
            table {
                border: 1px solid #000;
                padding: 5px;
            }
        </style>
    </head>
    <body>
        <table>
            <thead>
                <% header.forEach(head => { %>
                    <th><%= head %></th>
                <% }) %>
            </thead>
            <tbody>
                <% rows.forEach(row => { %>
                    <tr>
                        <% row.forEach(element => { %>
                            <td><%= element %> </td>
                        <% }) %>
                    </tr>
                <% }) %>
            </tbody>
        </table>
    </body>
</html>